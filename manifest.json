{
    "manifest_version": 3,
    "name": "YouTube Channel Finder",
    "version": "1.0",
    "description": "Find YouTube channels by name",
    "icons": {
        "16": "./icons/icon16.png",
        "48": "./icons/icon48.png",
        "128": "./icons/icon128.png"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "./icons/icon16.png",
        "48": "./icons/icon48.png",
        "128": "./icons/icon128.png"
      }
    },
    "host_permissions": [
      "http://localhost:4567/*"
    ]
  }